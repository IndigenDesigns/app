<button id="buttonaddcontact" type="button" class="btn btn-primary mb-2">
	<i class="fa fa-user-plus" aria-hidden="true"></i> New Contact
</button>
<table class="table table-striped">
	<thead>
		<tr>
			<th>Name</th>
			<th>SIP</th>
			<th>Actions</th>
		</tr>
	</thead>
	<tbody>
		<tr>
			<td>echo</td>
			<td>echo@studio.link</td>
			<td>
				<p>
				<button data-sip="echo@studio.link" class="btn btn-success btn-sm callcontact">
					<span class="glyphicon glyphicon-earphone"></span> Call
				</button>
				</p>
			</td>
		</tr>
		<tr>
			<td>music</td>
			<td>music@studio.link</td>
			<td>
				<p>
				<button data-sip="music@studio.link" class="btn btn-success btn-sm callcontact">
					<span class="glyphicon glyphicon-earphone"></span> Call
				</button>
				</p>
			</td>
		</tr>
		<tr>
			<td>music1</td>
			<td>music1@studio.link</td>
			<td>
				<p>
				<button data-sip="music1@studio.link" class="btn btn-success btn-sm callcontact">
					<span class="glyphicon glyphicon-earphone"></span> Call
				</button>
				</p>
			</td>
		</tr>

		{{#each this}}
		<tr>
			<td>{{name}}</td>
			<td>{{sip}}</td>
			<td>
				<p>
				<button data-sip="{{sip}}" class="btn btn-success btn-sm callcontact">
					<span class="glyphicon glyphicon-earphone"></span> Call
				</button>
				<button data-sip="{{sip}}" class="btn btn-danger btn-sm delete-button deletecontact">
					<span class="glyphicon glyphicon-remove-circle"></span> Delete
				</button>
				</p>
			</td>
		</tr>
		{{/each}}
	</tbody>
</table>
